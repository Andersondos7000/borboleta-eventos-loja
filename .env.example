# Configurações do Supabase
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Configurações do Gateway de Pagamento
# Configure aqui as variáveis do seu gateway de pagamento
# VITE_PAYMENT_API_KEY=your_api_key_here
# VITE_PAYMENT_WEBHOOK_ENDPOINT=your_webhook_endpoint_here
# VITE_PAYMENT_BASE_URL=https://api.payment-gateway.com/v1

# Ambiente
VITE_ENVIRONMENT=development

# URLs da aplicação
VITE_APP_URL=http://localhost:8080
VITE_API_URL=http://localhost:54321/functions/v1

# Configurações de desenvolvimento
VITE_DEBUG=true
VITE_LOG_LEVEL=debug

# Configurações de pagamento
VITE_PIX_EXPIRATION_MINUTES=30
VITE_PAYMENT_POLLING_INTERVAL=5000

# Configurações de webhook
VITE_WEBHOOK_SECRET=your-webhook-secret-here
VITE_WEBHOOK_TIMEOUT=30000