MCP_GATEWAY_HOST=0.0.0.0
MCP_LOG_LEVEL=info
MCP_ENABLE_CORS=true
MCP_ENABLE_WEBSOCKETS=true

# === APPLICATION CONFIGURATION ===
NODE_ENV=production
VITE_APP_NAME=Borboleta Eventos Loja
VITE_APP_VERSION=3.0.0
VITE_MCP_INTEGRATION=true

# === SECURITY ===
JWT_SECRET=your_jwt_secret_here_minimum_32_characters_long
API_RATE_LIMIT=1000
CORS_ORIGIN=*

# === MONITORING & HEALTH ===
HEALTH_CHECK_INTERVAL=30s
HEALTH_CHECK_TIMEOUT=10s
HEALTH_CHECK_RETRIES=3

# === PAYMENT INTEGRATION (AbacatePay) ===
ABACATE_PAY_API_KEY=your_abacate_pay_api_key_here
ABACATE_PAY_WEBHOOK_SECRET=your_webhook_secret_here

# === DEVELOPMENT ===
DEBUG=false
LOG_LEVEL=info
ENABLE_SWAGGER=false

# === DOCKER COMPOSE OVERRIDES ===
COMPOSE_PROJECT_NAME=borboleta-mcp
COMPOSE_HTTP_TIMEOUT=120
